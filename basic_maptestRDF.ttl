@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix map: <http://mapping.example.com/> .
@prefix biolink: <https://w3id.org/biolink/vocab/> .

map:jc_000 rr:child "ensemble_id" ;
	rr:parent "pmid" .

map:map_articles_000 rml:logicalSource map:source_000 ;
	rdf:type rr:TriplesMap ;
	rdfs:label "articles" ;
	rr:predicateObjectMap map:pom_000, map:pom_001 ;
	rr:subjectMap map:s_000 .

map:map_genes_000 rml:logicalSource map:source_001 ;
	rdf:type rr:TriplesMap ;
	rdfs:label "genes" ;
	rr:predicateObjectMap map:pom_002 ;
	rr:subjectMap map:s_001 .

map:om_000 rdf:type rr:ObjectMap ;
	rr:constant biolink:Article ;
	rr:termType rr:IRI .

map:om_001 rdf:type rr:ObjectMap ;
	rr:constant "https://w3id.org/biolink/vocab/Article" ;
	rr:termType rr:Literal .

map:om_002 rdf:type rr:ObjectMap ;
	rr:joinCondition map:jc_000 ;
	rr:parentTriplesMap map:map_genes_000 .

map:pm_000 rdf:type rr:PredicateMap ;
	rr:constant rdf:type .

map:pm_001 rdf:type rr:PredicateMap ;
	rr:constant biolink:mentions .

map:pm_002 rdf:type rr:PredicateMap ;
	rr:constant biolink:mentioned_by .

map:pom_000 rdf:type rr:PredicateObjectMap ;
	rr:objectMap map:om_000 ;
	rr:predicateMap map:pm_000 .

map:pom_001 rdf:type rr:PredicateObjectMap ;
	rr:objectMap map:om_002 ;
	rr:predicateMap map:pm_001 .

map:pom_002 rdf:type rr:PredicateObjectMap ;
	rr:objectMap map:om_001 ;
	rr:predicateMap map:pm_002 .

map:rules_000 <http://rdfs.org/ns/void#exampleResource> map:map_articles_000, map:map_genes_000 ;
	rdf:type <http://rdfs.org/ns/void#Dataset> .

map:s_000 rdf:type rr:SubjectMap ;
	rr:template "https://w3id.org/biolink/vocab/Article" .

map:s_001 rdf:type rr:SubjectMap ;
	rr:constant "https://w3id.org/biolink/vocab/Gene" .

map:source_000 rml:referenceFormulation ql:CSV ;
	rml:source "asd_article_metadata.csv" ;
	rdf:type rml:LogicalSource .

map:source_001 rml:referenceFormulation ql:CSV ;
	rml:source "dataset_test1.csv" ;
	rdf:type rml:LogicalSource .

