@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix map: <http://mapping.example.com/> .
@prefix dc-doi: <http://purl.org/spar/datacite/PrimaryResourceIdentifier> .
@prefix dc-has-id: <http://purl.org/spar/datacite/hasIdentifier> .
@prefix dc-has-desc: <http://purl.org/spar/datacite/hasDescription> .
@prefix dc-journal: <http://purl.org/spar/datacite/OrganizationIdentifierScheme> .
@prefix dc-date: <http://purl.org/dc/elements/1.1/date> .
@prefix ensemble-id: <http://edamontology.org/data_2610> .
@prefix has-output: <http://edamontology.org/has_output> .
@prefix has-id: <http://edamontology.org/has_identifier> .
@prefix NCBIGene: <http://edamontology.org/data_1027> .
@prefix article: <http://schema.org/Article> .
@prefix pmid: <http://identifiers.org/pmid/> .
@prefix doi: <https://doi.org/> .

map:jc_000 rr:child "ensemble_id" ;
	rr:parent "doi" .

map:map_articles_000 rml:logicalSource map:source_000 ;
	rdf:type rr:TriplesMap ;
	rdfs:label "articles" ;
	rr:predicateObjectMap map:pom_000, map:pom_001, map:pom_002, map:pom_003, map:pom_004 ;
	rr:subjectMap map:s_000 .

map:map_genes_000 rml:logicalSource map:source_001 ;
	rdf:type rr:TriplesMap ;
	rdfs:label "genes" ;
	rr:predicateObjectMap map:pom_005 ;
	rr:subjectMap map:s_001 .

map:om_000 rdf:type rr:ObjectMap ;
	rr:constant article:Article ;
	rr:termType rr:IRI .

map:om_001 rdf:type rr:ObjectMap ;
	rr:template "http://purl.org/spar/datacite/PrimaryResourceIdentifier{doi}" ;
	rr:termType rr:Literal .

map:om_002 rdf:type rr:ObjectMap ;
	rr:template "http://purl.org/spar/datacite/OrganizationIdentifierScheme{journal}" ;
	rr:termType rr:Literal .

map:om_003 rdf:type rr:ObjectMap ;
	rr:template "http://purl.org/dc/elements/1.1/date{year}" ;
	rr:termType rr:Literal .

map:om_004 rdf:type rr:ObjectMap ;
	rr:template "http://edamontology.org/data_1027{Entrez ID}" ;
	rr:termType rr:Literal .

map:om_005 rdf:type rr:ObjectMap ;
	rr:joinCondition map:jc_000 ;
	rr:parentTriplesMap map:map_genes_000 .

map:pm_000 rdf:type rr:PredicateMap ;
	rr:constant rdf:type .

map:pm_001 rdf:type rr:PredicateMap ;
	rr:constant <dc-has-id> .

map:pm_002 rdf:type rr:PredicateMap ;
	rr:constant <dc-has-desc> .

map:pm_003 rdf:type rr:PredicateMap ;
	rr:constant <dc-has-desc> .

map:pm_004 rdf:type rr:PredicateMap ;
	rr:constant <has-output> .

map:pm_005 rdf:type rr:PredicateMap ;
	rr:constant <has-id> .

map:pom_000 rdf:type rr:PredicateObjectMap ;
	rr:objectMap map:om_000 ;
	rr:predicateMap map:pm_000 .

map:pom_001 rdf:type rr:PredicateObjectMap ;
	rr:objectMap map:om_001 ;
	rr:predicateMap map:pm_001 .

map:pom_002 rdf:type rr:PredicateObjectMap ;
	rr:objectMap map:om_002 ;
	rr:predicateMap map:pm_002 .

map:pom_003 rdf:type rr:PredicateObjectMap ;
	rr:objectMap map:om_003 ;
	rr:predicateMap map:pm_003 .

map:pom_004 rdf:type rr:PredicateObjectMap ;
	rr:objectMap map:om_005 ;
	rr:predicateMap map:pm_004 .

map:pom_005 rdf:type rr:PredicateObjectMap ;
	rr:objectMap map:om_004 ;
	rr:predicateMap map:pm_005 .

map:rules_000 <http://rdfs.org/ns/void#exampleResource> map:map_articles_000, map:map_genes_000 ;
	rdf:type <http://rdfs.org/ns/void#Dataset> .

map:s_000 rdf:type rr:SubjectMap ;
	rr:template "https://doi.org/{doi}" .

map:s_001 rdf:type rr:SubjectMap ;
	rr:template "http://edamontology.org/data_2610{Ensemble ID}" .

map:source_000 rml:referenceFormulation ql:CSV ;
	rml:source "asd_article_metadata.csv" ;
	rdf:type rml:LogicalSource .

map:source_001 rml:referenceFormulation ql:CSV ;
	rml:source "dataset_test1.csv" ;
	rdf:type rml:LogicalSource .
